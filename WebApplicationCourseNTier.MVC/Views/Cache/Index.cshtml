@{
    string[] adSoyadlar = {
        "Ali Vəliyev",
        "Aysel Məmmədova",
        "Nigar Həsənli",
        "Rəşad Quliyev",
        "Elçin Əliyev"
    };
}

<!DOCTYPE html>
<html>
<head>
    <title>Ad və Soyadlar</title>
</head>
<body>
    <h1>Ad və Soyadlar</h1>
    <ul>
        @foreach (var adSoyad in adSoyadlar)
        {
            <li>@adSoyad</li>
        }
    </ul>
</body>
</html>